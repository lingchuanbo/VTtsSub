  "regex_patterns": {
    "_description": "正则表达式修正模式（高级用户使用）",
    "_note": "pattern 为正则表达式，replacement 为替换字符串，flags 可选",
    "patterns": [
      {
        "name": "fix_dev_space",
        "description": "修复 Dev 后面的空格问题",
        "pattern": "\\bDev\\s+(\\d+)",
        "replacement": "Dev$1",
        "flags": ""
      },
      {
        "name": "fix_version_space",
        "description": "修复版本号中的空格",
        "pattern": "(\\d+)\\. (\\d+)",
        "replacement": "$1.$2",
        "flags": ""
      },
      {
        "name": "fix_pr_case",
        "description": "确保 PR/PRs 大写",
        "pattern": "\\b[Pp][Rr]s?\\b",
        "replacement": "PR",
        "flags": "i"
      }
    ]
  }
}
